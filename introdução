// variável, seu valor pode ser alterado o decorrer do programa
let nomeEstudante = Helena; 
console.log(nomeEstudante);

// constate, seu valor não pode ser alterado
const nomeDoEstudante = "Helena";
console.log(nomeDoEstudante);

//Em outras liguagens, portanto, você encontrará algo semelhante a:
String: nomeEstudante = "Helena"